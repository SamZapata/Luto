<p id="notice"><%= notice %></p>

<h1>Tipo Answers</h1>

<table>
  <thead>
    <tr>
      <th>Topic</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_answers.each do |tipo_answer| %>
      <tr>
        <td><%= tipo_answer.topic %></td>
        <td><%= link_to 'Show', tipo_answer %></td>
        <td><%= link_to 'Edit', edit_tipo_answer_path(tipo_answer) %></td>
        <td><%= link_to 'Destroy', tipo_answer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Answer', new_tipo_answer_path %>
